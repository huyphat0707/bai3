{
  "name": "tool",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Extension",
  "icons": {
    "48": "img/icons/star.png"
  },
  "background": {
    "scripts": ["./js/scripts/background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "48": "img/icons/star.png"
    },
    "default_popup": "popup.html",
    "default_title": "Extension"
  },
  "permissions": [
    "clipboardWrite",
    "storage",
    "tabs",
    "activeTab",
    "http://*/*"
  ]
}
